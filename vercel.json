{
    "version": 2,
    "devCommand": "npm run dev",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}